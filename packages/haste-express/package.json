{
  "name": "@hastearcade/haste-express",
  "version": "1.1.0",
  "description": "The sdk to be used to empower developers to leverage the Haste Arcade. The SDK is intended to be used in an express based regular web appliaction alongside the developers game logic.",
  "contributors": [
    "Keith LaForce <keith@hastearcade.com>",
    "Eric LaForce <eric@hastearcade.com>",
    "Dan Wagner <dan@hastearcade.com>"
  ],
  "homepage": "https://hastearcade.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hastearcade/haste-sdk.git"
  },
  "files": [
    "dist"
  ],
  "source": "lib/index.js",
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.modern.js"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "scripts": {
    "semantic-release": "semantic-release",
    "build": "rm -rf dist/ && microbundle",
    "pretty": "",
    "lint": ""
  },
  "bugs": {
    "url": "https://github.com/hastearcade/haste-sdk/issues"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "passport": "^0.6.0",
    "passport-auth0": "^1.4.1",
    "uuid": "^8.3.2"
  },
  "keywords": [
    "haste",
    "game",
    "arcade",
    "hlp",
    "bsv",
    "payout"
  ],
  "devDependencies": {
    "@semantic-release/git": "^9.0.0",
